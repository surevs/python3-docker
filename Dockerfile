FROM python:3.7-slim-stretch
# Step 2
# Essential tools and xvfb
RUN apt update
RUN DEBIAN_FRONTEND="noninteractive" apt -y install tzdata
RUN apt install -y \
    xvfb \
    gnupg \
    libxml2-dev \
    zlib1g-dev \
    net-tools \
    unzip \
    build-essential \
    autoconf \
    libtool \
    pkg-config \
    libglib2.0-0 \
    apt-utils \
    libgl1-mesa-dev \
    libaspell-dev \
    aspell
# Install Chrome / Puppeteer dependencies
RUN apt install -y libpangocairo-1.0-0 \
    libx11-xcb1 libxcomposite1 libxdamage1 \
    libxi6 libxtst6 libnss3 libcups2 libxss1 \
    libxrandr2 libasound2 libatk1.0-0 libgtk-3-0 \
    wget
# Install Selenium driver dependency, Chrome, ChromeDriver, Chromium
# RUN apt -y install gnupg
RUN echo 'deb http://dl.google.com/linux/chrome/deb/ stable main' > /etc/apt/sources.list.d/google-chrome.list &&\
wget -q -O - https://dl-ssl.google.com/linux/linux_signing_key.pub | apt-key add - &&\
apt update &&\
apt -y install google-chrome-stable
RUN apt -y install unzip &&\
wget -q https://chromedriver.storage.googleapis.com/85.0.4183.87/chromedriver_linux64.zip &&\
unzip chromedriver_linux64.zip &&\
mv chromedriver /usr/bin/chromedriver &&\
chown root:root /usr/bin/chromedriver &&\
chmod +x /usr/bin/chromedriver &&\
rm chromedriver_linux64.zip
RUN apt -y install chromium
RUN apt-get install -y curl gcc g++ make
RUN curl -sL https://deb.nodesource.com/setup_12.x | bash - 
RUN apt install -y nodejs
RUN npm install -g lighthouse
